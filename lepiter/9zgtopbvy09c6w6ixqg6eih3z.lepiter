				"editEmail" : {
					"__type" : "email",
					"emailString" : "<unknown>"
				},
				"editTime" : {
					"__type" : "time",
					"time" : {
						"__type" : "dateAndTime",
						"dateAndTimeString" : "2021-09-07T10:20:37.005775+02:00"
					}
				},
				"uid" : {
					"__type" : "uid",
					"uidString" : "OFFSLcWHDQC26iMiAn+pqA=="
				},
				"paragraphStyle" : {
					"__type" : "textStyle"
				},
				"string" : "Aside: Images and captions present numerous inefficiencies and compromises which will require more than journal compression before they meet all expectations. "
			},
			{
				"__type" : "textSnippet",
				"children" : {
					"__type" : "snippets",
					"items" : [ ]
				},
